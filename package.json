  "engines": {
    "npm": "^3.0.0",
    "node": "^4.0.0"
  },
  "scripts": {
    "lint": "node_modules/.bin/eslint services/**",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate:revert": "node_modules/.bin/sequelize db:migrate:undo",
    "start": "node_modules/.bin/pm2 startOrGracefulReload process.yml",
    "restart": "node_modules/.bin/pm2 startOrGracefulReload process.yml",
    "stop": "node_modules/.bin/pm2 startOrRestart process.yml && node_modules/.bin/pm2 delete process.yml",
    "logs": "node_modules/.bin/pm2 logs",
    "beautify": "node_modules/.bin/esformatter -i 'services/**/*.js'",
    "test": "NODE_ENV=test node ./node_modules/mocha/bin/mocha --recursive spec/"
      //"codecov": "NODE_ENV=test istanbul cover ./node_modules/.bin/_mocha --recursive spec/* --report lcovonly -- -R spec && codecov -t CODECOV_TOKEN_HERE"
  },
