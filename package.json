    "pm2": "^1.1.0",
    "promise": "7.1.x",
    "redis": "^2.6.1",
    "request": "^2.69.0",
    "response-time": "2.3.x",
    "sequelize": "^3.23.4",
    "sequelize-cli": "^2.4.0",
    "serve-favicon": "2.2.x",
    "underscore": "^1.8.3"
  },
